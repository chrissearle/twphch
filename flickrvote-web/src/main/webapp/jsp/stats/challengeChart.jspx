<jsp:root xmlns:jsp="http://java.sun.com/JSP/Page"
          xmlns="http://www.w3.org/1999/xhtml"
          xmlns:s="/struts-tags"
          version="2.0" xmlns:fv="/flickrvote-tags">
    <jsp:directive.page contentType="text/html"/>
    <jsp:output omit-xml-declaration="yes"
                doctype-root-element="html"
                doctype-public="-//W3C//DTD XHTML 1.1//EN"
                doctype-system="http://www.w3.org/TR/xhtml1/DTD/xhtml1-strict.dtd"/>
    <html>
    <head>
        <s:head/>

        <s:action name="loadJQuery" namespace="/common" executeResult="true"/>
        <s:action name="loadHighcharts" namespace="/common" executeResult="true"/>
        <s:action name="challengeChartJs" namespace="/stats" executeResult="true">
            <s:param name="tag" value="challenge.challengeTag"/>
        </s:action>
    </head>
    <body>
    <h1><fv:challengeTitle challenge="${challenge}"/></h1>
    <div id="challengeChart" style="width: 500px; height: 1000px"></div>
    </body>
    </html>
</jsp:root>